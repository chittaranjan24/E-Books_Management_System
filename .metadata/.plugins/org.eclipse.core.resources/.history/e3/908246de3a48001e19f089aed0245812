package com.user.servlet;

import java.io.IOException;

import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

@WebServlet("/order")
public class OrderServlet extends HttpServlet{

	@Override
	protected void doPost(HttpServletRequest req, HttpServletResponse resp) throws ServletException, IOException {
		try {
			
			String name=req.getParameter("");
			String email=req.getParameter("");
			String phno=req.getParameter("");
			String address=req.getParameter("");
			String landmark=req.getParameter("");
			String city=req.getParameter("");
			String state=req.getParameter("");
			String pincode=req.getParameter("");
			String paymentType=req.getParameter("");
			
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
	
	

}
